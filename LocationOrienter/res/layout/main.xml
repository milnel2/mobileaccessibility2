<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout android:id="@+id/MainRelativeLayout01" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android" android:background="#FFFFFF">
	<HorizontalScrollView android:id="@+id/MainHorizontalScrollView01" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android" android:clickable="true" android:background="#FFFFFF" android:layout_width="wrap_content" android:layout_toRightOf="@+id/MainTableLayout01">
		<TextView android:id="@+id/MainTextView01" android:textColor="#000000" android:singleLine="true" android:ellipsize="none" android:layout_height="fill_parent" android:layout_width="fill_parent" android:layout_gravity="center_vertical" android:gravity="center_vertical" android:text="@+id/MainTextView01" android:textSize="95px"/>
	</HorizontalScrollView>

	<TableLayout android:id="@+id/MainTableLayout01" android:layout_width="wrap_content" android:layout_height="fill_parent">
		<TableRow android:id="@+id/MainTableRow01" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight=".3">
			<ImageButton android:layout_width="wrap_content" android:layout_alignParentLeft="true" android:textColor="#000000" android:layout_height="fill_parent" android:src="@drawable/zoomin" android:id="@+id/MainImageButton01"/>
		</TableRow>
		
		<TableRow android:id="@+id/MainTableRow02" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_gravity="center_vertical" android:layout_weight=".3">
			<ImageButton android:layout_width="wrap_content" android:layout_alignParentLeft="true" android:textColor="#000000" android:layout_height="fill_parent" android:src="@drawable/zoomout" android:id="@+id/MainImageButton02"/>
		</TableRow>
		
		<TableRow android:id="@+id/MainTableRow03" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_gravity="bottom" android:layout_weight=".3">
			<ImageButton android:layout_width="wrap_content" android:layout_alignParentLeft="true" android:textColor="#000000" android:layout_height="fill_parent" android:src="@drawable/speaker" android:id="@+id/MainImageButton03"/>
		</TableRow>
	</TableLayout>

</RelativeLayout>