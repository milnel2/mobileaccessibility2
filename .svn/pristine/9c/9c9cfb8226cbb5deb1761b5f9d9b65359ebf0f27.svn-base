<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/store"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<Button android:id="@+id/blank_button"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:focusableInTouchMode="true"
		android:background="@color/background"
		android:tag=""
		android:text=""
		android:textSize="1sp"/>
	<Button android:id="@+id/food_product"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:focusableInTouchMode="true"
		android:tag="@string/food_aisle"
		android:text="@string/food_product_label"
		android:textSize="30.0sp"
		android:layout_weight="2"/>
	<Button android:id="@+id/play_product"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:focusableInTouchMode="true"
		android:tag="@string/toy_aisle"
		android:text="@string/play_product_label"
		android:textSize="30.0sp"
		android:layout_weight="2"/>
	<Button android:id="@+id/care_product"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:focusableInTouchMode="true"
		android:tag="@string/care_aisle"
		android:text="@string/care_product_label"
		android:textSize="30.0sp"
		android:layout_weight="2"/>
	<Button android:id="@+id/accessory_product"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:focusableInTouchMode="true"
		android:tag="@string/accessory_aisle"
		android:text="@string/accessory_product_label"
		android:textSize="30.0sp"
		android:layout_weight="2"/>
	<Button android:id="@+id/exit_store"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:focusableInTouchMode="true"
		android:tag="@string/exit_store"
		android:text="@string/exit_store_label"
		android:textSize="30.0sp"
		android:layout_weight="2"/>		
</LinearLayout>
	